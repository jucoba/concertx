{
  "name": "concertx-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:styles -- --env production",
    "build:app": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:styles -- -w",
    "dev:app": "remix dev",
    "start": "run-p start:*",
    "start:app": "dotenv remix-serve build",
    "start:css": "npm run build:css",
    "generate:styles": "postcss styles/**/*.css --base styles --dir app/styles"
  },
  "dependencies": {
    "@headlessui/react": "1.7.7",
    "@heroicons/react": "2.0.13",
    "@remix-run/node": "1.10.1",
    "@remix-run/react": "1.10.1",
    "@remix-run/serve": "1.10.1",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "framer-motion": "^9.0.2",
    "isbot": "3.6.5",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix-utils": "6.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.10.1",
    "@remix-run/eslint-config": "1.10.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.9",
    "@types/lodash": "4.14.191",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "cssnano": "5.1.14",
    "dotenv-cli": "^7.0.0",
    "eslint": "8.31.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "postcss-cli": "10.1.0",
    "postcss-import": "15.1.0",
    "tailwindcss": "3.2.4",
    "tiny-invariant": "1.3.1",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
